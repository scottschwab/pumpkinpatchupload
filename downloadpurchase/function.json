{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "authLevel": "function",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get"
      ]
    },
    {
      "name": "pumpkinstore",
      "type": "cosmosDB",
      "direction": "in",
      "leaseCollectionName" : "leases",
      "connectionStringSetting": "pumpkinpatchcosmos",
      "databaseName": "pumpkin-orders",
      "collectionName": "purchaseorder",
      "sqlQuery" : "SELECT * FROM c WHERE c.type='pumpkin order'"
    },
    {
      "name": "outputblob",
      "type": "blob",
      "path": "orders/{name}",
      "connection": "pumpkinpatchblobs",
      "direction": "out"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "$return"
    }
  ]
}